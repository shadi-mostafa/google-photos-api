{
    "version": 2,
    "builds": [
        {
            "src": "/api/*.js",
            "use": "@now/node"
        },
        {
            "src": "/api/hello/*.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}